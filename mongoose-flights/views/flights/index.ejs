<%- include('../partials/header') %>
    <table>
      <thead>
        <tr>
          <th>Airline</th>
          <th>Departing Airport</th>
          <th>Flight No.</th>
          <th>Departs</th>
        </tr>
      </thead>
      <tbody>
        <% flights.forEach(function(m) { %>
          <tr>
            <td><%= m.airline %></td>
            <td><%= m.airport %></td>
            <td><%= m.flightNo %></td>
            <td><%= m.departs %></td>
            <td><a href="/flights/<%=m._id %>">DETAILS</a></td>
          </tr>
        <% }); %>
      </tbody>
    </table>
 <%- include('../partials/footer') %>